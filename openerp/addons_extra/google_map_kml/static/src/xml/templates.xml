    <t t-name="location_map.website_button_visited">

        <div class="btn-group oe_WidgetWebsiteButton">
            <button class="btn btn-sm btn-success">
                <span class="oe_unpublish">Not Published in Map</span>
                <span class="oe_publish">Published in Map</span>
            </button>
        </div>

    </t>
    
    <t t-name="location_map.website_button_selectimage">

        <div class="btn-group oe_WidgetWebsiteButton">
            <button class="btn btn-sm btn-success">
                <span class="oe_unpublish">Marker</span>
                <span class="oe_publish">Logo</span>
            </button>
        </div>

    </t>
    
	<t t-name="location_autocomplete">
	    <span t-att-class="'oe_form_field '+widget.widget_class" t-att-style="widget.node.attrs.style">
	        <t t-if="!widget.get('effective_readonly')">
	            <input id="location_autocomplete_field" 
	            	t-att-type="widget.password ? 'password' : 'text'"
	                t-att-id="widget.id_for_label"
	                t-att-tabindex="widget.node.attrs.tabindex"
	                t-att-autofocus="widget.node.attrs.autofocus"
	                t-att-placeholder="widget.node.attrs.placeholder"
	                t-att-maxlength="widget.field.size"
	                class="highlight_field"
	            /><img class="oe_field_translate oe_input_icon" t-if="widget.field.translate" t-att-src='_s + "/web/static/src/img/icons/terp-translate.png"' width="16" height="16" border="0"/>
	        </t>
	        <t t-if="widget.get('effective_readonly')">
	            <span class="oe_form_char_content"></span>
	        </t>
	    </span>
	</t>
</templates>