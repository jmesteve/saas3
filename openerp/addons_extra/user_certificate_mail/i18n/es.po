#. module: user_certificate_mail
#: model:email.template,body_html:user_certificate_mail.email_template_ssl
msgid "<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"		<p>Hello ${object.user.name}, I send you the ssl certificate.</p>\n"
"</div>"
msgstr "<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"		<p>Hola ${object.user.name}, te envío adjunto el certificado SSL.</p>\n"
"</div>"

#. module: user_certificate_mail
#: model:email.template,body_html:user_certificate_mail.email_template_ssl_password
msgid "<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"		<p>Hello ${object.user.name}, I send you the password for the ssl certificate.</p>\n"
"		<p>Password: ${object.password}</p>\n"
"		% for certificate in object.certification_authority.certificates:\n"
"		% if certificate.type == 'server':\n"
"		<p>Web: https://${certificate.commonname}</p>\n"
"		% endif\n"
"		% endfor\n"
"		\n"
"</div>"
msgstr "<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"		<p>Hola ${object.user.name}, te envío la contraseña del certificado SSL.</p>\n"
"		<p>Contraseña: ${object.password}</p>\n"
"		% for certificate in object.certification_authority.certificates:\n"
"		% if certificate.type == 'server':\n"
"		<p>Web: https://${certificate.commonname}</p>\n"
"		% endif\n"
"		% endfor\n"
"		\n"
"</div>"

